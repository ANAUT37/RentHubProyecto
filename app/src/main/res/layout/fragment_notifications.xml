<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blanco"
    tools:context=".view.fragments.EditProfileFragment" >

    <TextView
        android:id="@+id/tvWhatFragment8"
        android:layout_width="220dp"
        android:layout_height="56dp"
        android:background="@drawable/bg_title"
        android:elevation="8dp"
        android:fontFamily="@font/roboto_bold"
        android:gravity="center"
        android:text="Notificaciones"
        android:textAlignment="center"
        android:textColor="@color/azulMarino"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/tvWhatFragment8"
        tools:layout_editor_absoluteX="0dp">

        <Button
            android:id="@+id/bttnNotiOpenSettings"
            android:layout_width="310dp"
            android:layout_height="45dp"
            android:layout_marginStart="44dp"
            android:background="#00FFFFFF"
            android:elevation="0dp"
            android:fontFamily="@font/roboto_medium"
            android:paddingStart="8dp"
            android:text="Ajustes del dispositivo"
            android:textAlignment="textStart"
            android:textAllCaps="false"
            android:textColor="@color/negroMedio"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/regTexto11"
            tools:ignore="TouchTargetSizeCheck" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="33dp"
            android:layout_marginEnd="36dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view13"
            app:srcCompat="@drawable/angle_right" />

        <TextView
            android:id="@+id/regTexto11"
            android:layout_width="wrap_content"
            android:layout_height="43dp"
            android:layout_marginStart="31dp"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/roboto_bold"
            android:gravity="center_vertical"
            android:text="Ajustes avanzados"
            android:textColor="@color/azulMarino"
            android:textColorHint="#656565"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView26" />

        <View
            android:id="@+id/view13"
            android:layout_width="0dp"
            android:layout_height="2dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="53dp"
            android:layout_marginEnd="36dp"
            android:background="@color/azulMedio"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/regTexto11"
            app:layout_constraintTop_toBottomOf="@+id/swNotiApp" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_marginStart="44dp"
            android:background="#00FFFFFF"
            android:elevation="0dp"
            android:fontFamily="@font/roboto_medium"
            android:gravity="center_vertical"
            android:paddingStart="8dp"
            android:text="De RêntHûb"
            android:textAlignment="textStart"
            android:textAllCaps="false"
            android:textColor="@color/negroMedio"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView24"
            tools:ignore="TouchTargetSizeCheck" />

        <Switch
            android:id="@+id/swNotiApp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="36dp"
            android:background="@drawable/bg_input_1"
            android:checked="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/swNotiFavoritos"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_marginStart="44dp"
            android:background="#00FFFFFF"
            android:elevation="0dp"
            android:fontFamily="@font/roboto_medium"
            android:gravity="center_vertical"
            android:paddingStart="8dp"
            android:text="@string/valoraciones"
            android:textAlignment="textStart"
            android:textAllCaps="false"
            android:textColor="@color/negroMedio"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView23"
            tools:ignore="TouchTargetSizeCheck" />

        <Switch
            android:id="@+id/swNotiFavoritos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="36dp"
            android:background="@drawable/bg_input_1"
            android:checked="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/swNotiValoraciones"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_marginStart="44dp"
            android:background="#00FFFFFF"
            android:elevation="0dp"
            android:fontFamily="@font/roboto_medium"
            android:gravity="center_vertical"
            android:paddingStart="8dp"
            android:text="Favoritos"
            android:textAlignment="textStart"
            android:textAllCaps="false"
            android:textColor="@color/negroMedio"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView25"
            tools:ignore="TouchTargetSizeCheck" />

        <Switch
            android:id="@+id/swNotiValoraciones"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="36dp"
            android:background="@drawable/bg_input_1"
            android:checked="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/swNotiMensajes"
            tools:ignore="TouchTargetSizeCheck" />

        <View
            android:id="@+id/view11"
            android:layout_width="0dp"
            android:layout_height="2dp"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="36dp"
            android:layout_marginBottom="18dp"
            android:background="@color/azulMedio"
            app:layout_constraintBottom_toTopOf="@+id/textView61"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/regTexto9" />

        <TextView
            android:id="@+id/regTexto9"
            android:layout_width="wrap_content"
            android:layout_height="43dp"
            android:layout_marginStart="31dp"
            android:fontFamily="@font/roboto_bold"
            android:gravity="center_vertical"
            android:text="Notificaciones push"
            android:textColor="@color/azulMarino"
            android:textColorHint="#656565"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView61"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_marginStart="45dp"
            android:background="#00FFFFFF"
            android:elevation="0dp"
            android:fontFamily="@font/roboto_medium"
            android:gravity="center_vertical"
            android:paddingStart="8dp"
            android:text="Pausar todas"
            android:textAlignment="textStart"
            android:textAllCaps="false"
            android:textColor="@color/negroMedio"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/regTexto9"
            tools:ignore="TouchTargetSizeCheck" />

        <Switch
            android:id="@+id/swNotiPauseAll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="29dp"
            android:layout_marginEnd="36dp"
            android:background="@drawable/bg_input_1"
            android:checked="false"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view11"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_marginStart="44dp"
            android:background="#00FFFFFF"
            android:elevation="0dp"
            android:fontFamily="@font/roboto_medium"
            android:gravity="center_vertical"
            android:paddingStart="8dp"
            android:text="Mensajes"
            android:textAlignment="textStart"
            android:textAllCaps="false"
            android:textColor="@color/negroMedio"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView61"
            tools:ignore="TouchTargetSizeCheck" />

        <Switch
            android:id="@+id/swNotiMensajes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="36dp"
            android:background="@drawable/bg_input_1"
            android:checked="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/swNotiPauseAll"
            tools:ignore="TouchTargetSizeCheck" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>