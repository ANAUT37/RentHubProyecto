<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_claro_transparente"
    tools:context=".view.fragments.UploadContratoFragment">

    <ImageView
        android:id="@+id/ivFotosAcordeon2"
        android:layout_width="0dp"
        android:layout_height="180dp"
        android:layout_marginTop="20dp"
        android:padding="70dp"
        android:src="@drawable/app_logo_01"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/app_logo_01" />

    <TextView
        android:id="@+id/tvWhatFragment11"
        android:layout_width="220dp"
        android:layout_height="56dp"
        android:background="@drawable/bg_title"
        android:elevation="8dp"
        android:fontFamily="@font/roboto_bold"
        android:gravity="center"
        android:text="Subir contrato"
        android:textAlignment="center"
        android:textColor="@color/azulMarino"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvErrorMessageNewContrato"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/bg_error_2"
        android:fontFamily="@font/roboto_bold"
        android:paddingTop="2dp"
        android:paddingBottom="2dp"
        android:text="Ups! Ha ocurrido un error."
        android:textAlignment="center"
        android:textColor="@color/blanco"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/bg_home"
        android:elevation="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvWhatFragment11">

        <Button
            android:id="@+id/bttnUploadContratoPersonalizado"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginStart="229dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="24dp"
            android:background="@drawable/bg_button_03"
            android:fontFamily="@font/roboto_medium"
            android:text="CONTINUAR"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.4"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/regTexto17"
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:layout_marginStart="31dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="38dp"
            android:layout_marginBottom="5dp"
            android:fontFamily="@font/roboto_bold"
            android:gravity="center_vertical"
            android:text="Nuevo Contrato"
            android:textColor="@color/azulMarino"
            android:textColorHint="#656565"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/etUploadTitulo"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/etNewContratoTitle"
            android:layout_width="0dp"
            android:layout_height="49dp"
            android:layout_marginStart="31dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="32dp"
            android:background="@drawable/bg_input_1"
            android:ems="10"
            android:fontFamily="@font/roboto_medium"
            android:hint="Título del contrato"
            android:imeOptions="flagNavigateNext"
            android:inputType="textPersonName"
            android:overScrollMode="always"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:shadowColor="#AEAEAE"
            android:textAlignment="textStart"
            android:textColor="@color/black"
            android:textColorHint="#656565"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/regTexto17" />

        <EditText
            android:id="@+id/etNewContratoDescripcion"
            android:layout_width="0dp"
            android:layout_height="150dp"
            android:layout_marginStart="31dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:background="@drawable/bg_input_1"
            android:ems="10"
            android:fontFamily="@font/roboto_medium"
            android:gravity="top"
            android:hint="Descripción del contrato"
            android:inputType="textMultiLine"
            android:padding="12dp"
            android:paddingEnd="10dp"
            android:shadowColor="#AEAEAE"
            android:textAlignment="textStart"
            android:textColor="@color/black"
            android:textColorHint="#656565"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etNewContratoTitle"
            tools:ignore="RtlCompat" />

        <View
            android:id="@+id/view29"
            android:layout_width="0dp"
            android:layout_height="2dp"
            android:layout_marginStart="31dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="32dp"
            android:background="@color/azulMedio"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etNewContratoDescripcion" />

        <TextView
            android:id="@+id/regTexto18"
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:layout_marginStart="31dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="38dp"
            android:fontFamily="@font/roboto_bold"
            android:gravity="center_vertical"
            android:text="Selecciona un archivo"
            android:textColor="@color/azulMarino"
            android:textColorHint="#656565"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view29" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="31dp"
            android:layout_marginEnd="31dp"
            android:fontFamily="@font/roboto_bold"
            android:text="Este archivo debe estar en formato PDF"
            android:textAlignment="center"
            android:textColor="@color/negroMedio"
            android:textColorHint="#656565"
            android:textSize="12sp"
            android:textStyle="italic"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/regTexto18" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="31dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="31dp"
            android:background="@drawable/bg_input_1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView31">

            <ImageView
                android:id="@+id/ivAnunItemFoto4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/file_pdf" />

            <View
                android:id="@+id/view30"
                android:layout_width="2dp"
                android:layout_height="0dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="32dp"
                android:background="@color/azulClaro"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/ivAnunItemFoto4"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tvNewContratoFileName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="8dp"
                android:fontFamily="@font/roboto_bold"
                android:gravity="center_vertical"
                android:hint="Ningún archivo seleccionado"
                android:textColor="@color/azulMarino"
                android:textColorHint="@color/grisClaro"
                android:textSize="14sp"
                android:textStyle="italic"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/view30"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/bttnContratoSubirPersonalizadoSeleccionar"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:background="@drawable/bg_button_03"
                android:elevation="0dp"
                android:fontFamily="@font/roboto_black"
                android:paddingStart="8dp"
                android:paddingEnd="8dp"
                android:text="Elegir contrato"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="@color/blanco"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/view30"
                app:layout_constraintTop_toBottomOf="@+id/tvNewContratoFileName"
                tools:ignore="TouchTargetSizeCheck" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>